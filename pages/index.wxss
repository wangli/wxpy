/* pages/index.wxss */
.goBtn{
   border:solid 1px #555555;
   border-radius: 50px;
   width: 50px;
   height: 50px;
   margin: auto;
   text-align: center;
   line-height: 43px;
   margin-top: 50%;
}
.goBtn:active{
   border:solid 1px #eeeeee;
   background-color: #f1f1f1;
}
.icon-circle{
   width:5px;
   height: 5px;
   border-radius: 50%;
   display: inline-block;
   background-color: #555555;
}