.winBtn {
   margin: 5vw;
   color: #fff;
   background-color: rgba(84, 110, 122, 1) !important;
   border: none;
   position: fixed;
   bottom: 0;
   width: 90vw;
}
.tit_a {
   padding-left: 5px;
}

.tit_a .txt12 {
   color: #aaa;
}

.stBtn {
   overflow: hidden;
   height: 36px;
   border-radius: 18px;
   border: none;
   color: #fff;
   background-color: rgba(84, 110, 122, 1);
   font-size: 14px;
   padding: 0 20px;
   position: relative;
   line-height: 36px;
}

.stBtn view {
   display: inline-block;
}

.stBtn .icon {
   margin-right: 5px;
}

.stBtn.icon {
   margin-right: 8px;
}

.upfileBtn {
   display: inline-block;
   color: #39b0ec;
   line-height:1.3;
   vertical-align: middle;
}

.upfileBtn icon {
   vertical-align: middle;
   font-size: 16px;
   color: #39b0ec;
}

.wc-input {
   height: 50px;
   color: rgba(255, 255, 255, 1);
   position: relative;
   font-size: 14px;
   padding: 0 25px;
   text-align: center;
}

.wc-placeholder {
   color: rgba(255, 255, 255, 0.5);
   font-weight: normal;
}

.wc-input:before {
   content: " ";
   height: 196%;
   width: 198%;
   position: absolute;
   left: 0;
   top: 0;
   color: #e5e5e5;
   -webkit-transform-origin: 0 0;
   transform-origin: 0 0;
   z-index: 2;
   border: 1px solid #fff;
   border-radius: 50px;
   -webkit-transform: scale(0.5);
   transform: scale(0.5);
}

.wc-button, .wc-button-m {
   height: 50px;
   font-size: 14px;
   line-height: 50px;
   padding: 0 25px;
   overflow: hidden;
   border: none;
   border-radius: 50px;
   background-color: #33d398;
   border-color: #33d398;
   color: #fff;
   text-align: center;
}

.wc-button-m {
   height: 30px;
   padding: 0 12px;
   border-radius: 15px;
   line-height: 30px;
}

.wc-button.disabled {
   opacity: 0.5;
}

.wc-button-hover {
   background-color: #27c78c;
}

.wc-button::after, .wc-button::before, .wc-button-m:after, .wc-button-m:before {
   border: none;
}

/*新增内容按钮  */

.newBtn {
   background-color: #555555;
   width: 60px;
   height: 60px;
   position: fixed;
   right: 12px;
   bottom: 12px;
   border-radius: 50%;
   overflow: hidden;
   line-height: 55px;
   text-align: center;
   color: #fff;
   box-shadow: 0 0 12px #888;
}
.newBtn:active{
   background-color: #333333;
   box-shadow: 0 0 22px #888;
}
.newBtn icon {
   font-size: 22px;
}

.btn-circle{
   display: inline-block;
   position: relative;
   width: 60px;
   height: 60px;
   border-radius: 50%;
   line-height: 50px;
   text-align: center;
}
.btn-circle:before{
   content: "";
   position: absolute;
   top: -52%;
   left: -52%;
   width: 200%;
   height: 200%;
   border-radius: 50%;
   border: 1px solid #cccccc;
   -webkit-transform: scale(0.5);
   transform: scale(0.5);
   z-index: 2;
}
.btn-circle icon{
   font-size: 20px;
   color: #999;
   height: 20px;
}
.btn-circle:active{
   background-color: #f1f1f1;
}
.btn-circle:active:before{
   border: 1px solid #aaa;
}
.btn-circle .ssc-icon-chevron-left{
   transform:rotate(180deg);
   position: relative;
   top:8px;
   left:2px;
}

/* 头像圆形图片 */
.u-avatar, .u-avatar-m, .u-avatar-s, .u-avatar-l {
   position: relative;
   width: 32px;
   height: 32px;
   border-radius: 50%;
   overflow: hidden;
   background-color: #f1f1f1;
}

.u-avatar-s {
   width: 24px;
   height: 24px;
}

.u-avatar-m {
   width: 50px;
   height: 50px;
}

.u-avatar-l {
   width: 100px;
   height: 100px;
}

